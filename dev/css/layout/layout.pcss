@import "global.pcss";

.wrap {
  @mixin wrap1200;
  @mixin clearfix;
  h1 {
    @mixin fl;
    line-height: 60px;
    font-size: 30px;
  }
}

.navlist {
  @mixin fr;
  margin: 20px;
  li {
    @mixin fl;
    padding-left: 100px;
    line-height: 60px;
    a {
      font-size: 14px;
      color: #000;
      text-decoration: none;
    }
  }
}

.footer {
  background: #f0f0f0;
  p {
    padding: 50px 150px;
    font-size: 20px;
  }
}
