@import "global.pcss";

.wrap {
    @mixin wrap1200;
    @mixin clearfix;
    h1 {
        @mixin fl;
        line-height: 60px;
        font-size: 30px;
    }
}

.navlist {
    @mixin fr;
    li {
        @mixin fl;
        padding-left: 100px;
        line-height: 60px;
        font-size: 14px;
    }
}